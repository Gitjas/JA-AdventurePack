IF
	See(Player1)
	HPPercentLT(Player1,100)
	Global("JA#NASHA_HEAL","LOCALS",1)
THEN
	RESPONSE #100
		SaveObjectLocation("LOCALS","JA#NASHA_POS",Myself)
		MoveToObject(Player1)
		Wait(1)
		ReallyForceSpell(Player1,CLERIC_HEAL)
		Wait(2)
		MoveToSavedLocationn("JA#NASHA_POS","LOCALS")
		FaceObject([PC])
END

