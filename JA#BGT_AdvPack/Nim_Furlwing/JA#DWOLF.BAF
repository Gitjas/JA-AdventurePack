
IF
	AttackedBy([ANYONE],DEFAULT)
THEN
	RESPONSE #100
		AttackReevaluate(LastAttackerOf(Myself),90)
END

IF
	See("JA#HOUND")
	Range(LastSeenBy(Myself),20)
THEN
	RESPONSE #100
		AttackReevaluate(LastSeenBy(Myself),300)
	RESPONSE #100
		Continue()
END

IF
	See(NearestEnemyOf(Myself))
THEN
	RESPONSE #100
		AttackReevaluate(NearestEnemyOf(Myself),90)
END

