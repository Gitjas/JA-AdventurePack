IF
	GlobalLT("JA#ELFSONG_QUEST","GLOBAL",9)
	Global("JA#NEWCUSTOMER","LOCALS",0)
THEN
	RESPONSE #100
		SetGlobal("JA#NEWCUSTOMER","LOCALS",1)
		Deactivate(Myself)
END

IF
	GlobalGT("JA#ELFSONG_QUEST","GLOBAL",8)
	Global("JA#NEWCUSTOMER","LOCALS",1)
THEN
	RESPONSE #100
		SetGlobal("JA#NEWCUSTOMER","LOCALS",2)
		Activate(Myself)
END